{
  "name": "rai",
  "displayName": "Rai",
  "publisher": "HichemDahi",
  "description": "Refactor ai",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.98.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "activationEvents": [
    "onStartupFinished"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hichem-dahi/rai"
  },
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "similarityViewContainer",
          "title": "Code Similarity",
          "icon": "assets/similarity-icon.svg"
        }
      ]
    },
    "views": {
      "similarityViewContainer": [
        {
          "id": "similarityExplorer",
          "name": "Similarities",
          "icon": "assets/similarity-icon.svg"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "rai.analyzeSimilarity",
          "when": "view == similarityExplorer",
          "group": "navigation@1"
        }
      ]
    },
    "commands": [
      {
        "command": "rai.analyzeSimilarity",
        "title": "Analyze",
        "icon": "$(rocket)"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/emscripten": "^1.40.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.98.0",
    "@types/webassembly-js-api": "^0.0.3",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.21.0",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@electric-sql/pglite": "^0.2.17",
    "@xenova/transformers": "^2.17.2"
  },
  "files": [
    "out",
    "src/models"
  ]
}